<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".circlepercent.CirclePercentActivity">


    <!--
        android:background="@android:color/darker_gray"-->
    <mo.wall.org.circlepercent.CirclePercentLineView
        android:id="@+id/cpv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/w_dp_300"
        android:visibility="visible" />

    <mo.wall.org.circlepercent.ColumnarView
        android:id="@+id/cv"
        android:layout_width="match_parent"
        android:padding="@dimen/w_dp_1"
        android:layout_height="@dimen/w_dp_250"
        android:visibility="gone" />
</LinearLayout>